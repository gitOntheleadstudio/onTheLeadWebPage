{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/footer.svg",
        "destination": "/footer.svg"
      },
      {
        "source": "/lrv.png",
        "destination": "./lrv.png"
      }
    ]
  }
}
